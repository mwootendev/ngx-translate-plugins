{
  "name": "ngx-translate-plugins",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run build:testing && yarn run build:l10n && yarn run build:demo",
    "build:demo": "ng build",
    "build:testing": "ng build testing",
    "build:testing:prod": "yarn run build:testing --prod",
    "build:l10n": "ng build l10n",
    "build:l10n:prod": "yarn run build:l10n --prod",
    "clean": "rimraf dist coverage",
    "coverage:demo": "yarn run test:demo --code-coverage true",
    "coverage:testing": "yarn run test:testing --code-coverage true",
    "coverage:l10n": "yarn run test:l10n --code-coverage true",
    "pretest": "yarn run build:testing",
    "test": "yarn run test:testing --single-run true && yarn run test:demo --single-run true",
    "test:demo": "ng test --project=ngx-translate-plugins",
    "test:testing": "ng test --project=testing",
    "test:l10n": "ng test --project=l10n",
    "lint": "yarn run lint:testing && yarn run lint:l10n && yarn run lint:demo",
    "lint:demo": "ng lint --project=ngx-translate-plugins",
    "lint:testing": "ng lint --project=testing",
    "lint:l10n": "ng lint --project=l10n",
    "e2e": "ng e2e",
    "precommit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/animations": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.4",
    "messageformat": "^2.0.2",
    "ngx-translate-messageformat-compiler": "^4.1.3",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular-devkit/build-angular": "~0.6.8",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
